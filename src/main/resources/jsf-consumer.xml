<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jsf="http://jsf.jd.com/schema/jsf"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd">

    <!--For SSO -->
    <jsf:registry id="jsfRegistry" index="i.jsf.jd.com"/>

    <!-- 单点登录JSF服务接口 -->
    <jsf:consumer id="ssoService"
                  interface="com.jd.ssa.service.SsoService" protocol="jsf"
                  alias="v1" timeout="10000"/>

    <!-- 单点登录JSF服务接口 -->
    <jsf:consumer id="processCenterService"
                  interface="com.jd.oa.api.service.ProcessCenterService"
                  alias="pcsjsf" protocol="jsf" timeout="50000"></jsf:consumer>


